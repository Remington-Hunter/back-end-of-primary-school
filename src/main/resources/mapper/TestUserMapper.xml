<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="summer.project.mapper.TestUserMapper">
    <select id="queryUserByName" parameterType="String" resultType="summer.project.entity.TestUser">
